{
    "manifest_version": 3,
    "name": "CRXJS Vue Vite Example",
    "version": "1.0.0",
    "action": {
        "default_popup": "src/index.html"
    },
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.dlsite.com/*/fsr/*"
            ],
            "js": [
                "./src/contents/dliste_search.ts"
            ],
            "run_at": "document_end"
        }
    ]
}